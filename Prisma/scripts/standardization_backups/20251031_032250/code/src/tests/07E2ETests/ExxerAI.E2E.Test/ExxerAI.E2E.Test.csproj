	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="../../../Core/ExxerAI.Application/ExxerAI.Application.csproj" />
		<ProjectReference Include="../../../Core/ExxerAI.Domain/ExxerAI.Domain.csproj" />
	</ItemGroup>
</Project>
